{
    "name": "lf2-highlight",
    "displayName": "LF2 syntax highlight",
    "description": "Colorizing syntaxes of LF2 data file.",
    "version": "0.0.1",
    "publisher": "JohnDoeAntler",
    "repository": {
        "url": "https://github.com/JohnDoeAntler/LF2-highlight.git"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lf2-dat",
            "aliases": ["LF2 Data File"],
            "extensions": [".dat"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lf2-dat",
            "scopeName": "source.dat",
            "path": "./syntaxes/lf2-dat.tmLanguage.json"
        }]
    }
}